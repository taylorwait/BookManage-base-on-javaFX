<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.112" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.control.PurchaseManageControl">
    <children>
        <Pane layoutX="1.0" layoutY="-2.0" prefHeight="400.0" prefWidth="600.0">
            <children>
                <Button layoutX="56.0" layoutY="126.0" mnemonicParsing="false" prefHeight="50.0" prefWidth="89.0" text="进货记录"
                        onAction="#openPurchaseRecord"/>
                <Button layoutX="242.0" layoutY="126.0" mnemonicParsing="false" prefHeight="50.0" prefWidth="89.0" text="供应商"
                        onAction="#openSupplier"/>
                <Button layoutX="447.0" layoutY="126.0" mnemonicParsing="false" prefHeight="50.0" prefWidth="89.0" text="退货记录"
                       onAction="#openReturnRecord"/>
                <Button layoutX="56.0" layoutY="234.0" mnemonicParsing="false" prefHeight="50.0" prefWidth="89.0" text="仓库"
                        onAction="#openStore"/>
                <Label layoutX="99.0" layoutY="34.0" prefHeight="71.0" prefWidth="403.0" text="      进货管理" />
            </children>
        </Pane>
    </children>
    <stylesheets>
        <URL value="@purchaseManage.css" />
    </stylesheets>
</AnchorPane>
